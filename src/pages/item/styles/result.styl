@import "../../../assets/variables.styl"
.result
  height 100vh
  background-color #fff
  &_icon
    font-size .86rem
    color $mainColor
    padding .95rem .75rem .75rem
  &_status
    font-size .36rem
    line-height .43rem
    color $mainColor0
  &_tips
    font-size .28rem
    color $mainColor2
    margin-top .4rem
    padding 0 .32rem
  &_btn_group
    margin-top .7rem
    padding 0 .32rem
    .weui_btn
      width 686rpx
      font-size 32rpx
      line-height 88rpx
      color rgba(255,255,255,1)
      border-radius 8rpx
      text-align center
    .weui_btn_primary
      background-color $mainColor
    .weui_btn_default
      margin-top 35rpx
      background-color rgba(240,240,240,1)
      color rgba(35,35,35,1)
