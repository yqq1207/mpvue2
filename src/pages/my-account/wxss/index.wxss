.account-index{
  box-sizing: border-box;
  width: 100vw;
  padding-left: 18rpx;
  padding-right: 18rpx;
}
.index-header{
  width: 100%;
  height: 148rpx;
  /* line-height: 148rpx; */
}
.index-logo{
  width: 110rpx;
  height: 90rpx;
  margin-top: 28rpx;
}
.header-right{
  box-sizing: border-box;
  padding-top: 24rpx;
  padding-bottom: 50rpx;
  height: 148rpx;
}
.my-account-text{
  /* border-bottom: ; */
  margin: 0 22rpx 10rpx 72rpx;
}
.my-account-border{
  border-bottom: 1px solid #00A4A2; height: 10rpx; width: 66rpx;margin: 0 auto;
}
.accpunt-admin{
  width: 714rpx;
  height: 212rpx;
  border-radius: 6rpx;
  box-shadow: 0rpx 2rpx 6rpx #999;
  padding: 36rpx 40rpx 36rpx;
  box-sizing: border-box;
}
.admin-right{
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
}
.admin-left{
  max-width: 570rpx;
  display: block;
  overflow:hidden;
  text-overflow:ellipsis;
}
.admin-nike-img{
  width: 140rpx;
  height: 140rpx;
  border: 1px solid #fff;
  border-radius: 50%;
}
.content-list{
  box-sizing: border-box;
  width: 100%;
  padding: 32rpx 28rpx 34rpx 44rpx;
  height: 100rpx;
  border-radius: 10rpx;
  box-shadow: 0 0rpx 2rpx #999;
}
